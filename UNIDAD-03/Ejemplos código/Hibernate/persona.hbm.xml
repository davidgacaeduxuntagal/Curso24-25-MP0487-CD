<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="neoqubit.net.pruebasMaven.entities.Persona" table="personas">
		<id name="id" type="integer" column="id">
			<generator class="identity"></generator>
		</id>
		<property name="nombre" column="nombre"></property>
		<property name="edad" column="edad"></property>
		<property name="fechaNacimiento" column="fechaNacimiento"></property>
	</class>
</hibernate-mapping>